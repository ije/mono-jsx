{
  "name": "mono-jsx",
  "version": "0.8.0-beta.9",
  "description": "`<html>` as a `Response`.",
  "type": "module",
  "module": "./index.mjs",
  "types": "./types/index.d.ts",
  "bin": {
    "mono-jsx": "bin/mono-jsx"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "node": "./index.mjs",
      "import": "./index.mjs"
    },
    "./jsx-runtime": {
      "types": "./types/jsx-runtime.d.ts",
      "node": "./jsx-runtime.mjs",
      "import": "./jsx-runtime.mjs"
    },
    "./jsx-dev-runtime": {
      "types": "./types/jsx-runtime.d.ts",
      "node": "./jsx-runtime.mjs",
      "import": "./jsx-runtime.mjs"
    },
    "./dom/jsx-runtime": {
      "types": "./types/dom/jsx-runtime.d.ts",
      "node": "./dom/jsx-runtime.mjs",
      "import": "./dom/jsx-runtime.mjs"
    },
    "./dom/jsx-dev-runtime": {
      "types": "./types/dom/jsx-runtime.d.ts",
      "node": "./dom/jsx-runtime.mjs",
      "import": "./dom/jsx-runtime.mjs"
    }
  },
  "scripts": {
    "prepublishOnly": "deno task build"
  },
  "files": [
    "./*.mjs",
    "./dom/*.mjs",
    "./types/*.d.ts",
    "./types/dom/*.d.ts"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ije/mono-jsx.git"
  }
}
